{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "husky",
    "typecheck": "nuxt prepare && vue-tsc -b --noEmit",
    "release": "npx changelogen@latest --release --push",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.58",
    "@iconify-json/simple-icons": "^1.2.44",
    "@nuxt/image": "1.11.0",
    "@nuxt/ui": "3.3.0",
    "nuxt": "4.0.3"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.7.1",
    "@nuxt/test-utils": "3.19.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.32.0",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.4",
    "playwright-core": "^1.54.2",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.14.0",
  "lint-staged": {
    "app/**/*.{ts,vue}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "version": "0.0.3"
}
